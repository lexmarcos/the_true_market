# Exemplo de arquivo de configuração
# Copie este arquivo para .env e ajuste as configurações

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest

# RabbitMQ Exchange
# Exchange do tipo topic para roteamento baseado em padrões
RABBITMQ_EXCHANGE=skin.market.data

# ============================
# BitSkins Bot Configuration
# ============================
BITSKINS_ENABLED=true
BITSKINS_MIN_DISCOUNT=55
BITSKINS_CHECK_INTERVAL=60
BITSKINS_SEARCH_KNIVES=true
BITSKINS_SEARCH_WEAPONS=true
# Price Filters (for weapons only, in cents)
BITSKINS_PRICE_FROM=10000
BITSKINS_PRICE_TO=25000000

# ============================
# DashSkins Bot Configuration
# ============================
DASHSKINS_ENABLED=true
DASHSKINS_MIN_DISCOUNT=30
DASHSKINS_CHECK_INTERVAL=60
